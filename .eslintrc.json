{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:react/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extension": [".ts", ".tsx"]}],
        "react/props-types": ["error", {"ignore": ["navigation", "navigation.navigate"]}],
        "react/display-name": "off"
        
    }
}
